{
  "name": "digdir-docs-portal",
  "version": "0.1.0",
  "description": "Multi-product documentation portal for Digdir products",
  "private": true,
  "scripts": {
    "search:index": "node ./search/indexer.mjs",
    "search:index:fast": "node ./search/indexer.mjs --maxFiles 50",
    "search:api": "tsx ./search/server/server.ts",
    "docs:dev": "hugo server --source hugo",
    "docs:build": "hugo --source hugo"
  },
  "dependencies": {
    "express": "^4.19.2",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.0.0"
  }
}
